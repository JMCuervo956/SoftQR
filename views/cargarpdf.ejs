        <input type="text" name="empresaId" value="Emp4" placeholder="Nombre de la empresa"> <!-- Cambia esto segÃºn la empresa -->
        <input type="file" name="file" accept=".pdf" required />
        <button type="submit">Subir PDF</button>
    </form>

</body>
